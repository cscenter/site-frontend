<!DOCTYPE html>
<html lang="ru">
<head>
    <meta charset="utf-8">
    <title>{% block title %}Цели и история центра{% endblock title %}</title>
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description"
          content="Computer Science Center — объединённая инициатива Школы анализа данных, Computer Science клуба и JetBrains.">
    <meta name="google-site-verification" content="pBs4x-E9VGlX-gA7WxCNEqcYB0TYJCSmcZtSeLBpEgk"/>

    <link rel="shortcut icon" type="image/png" href="{{ static("v2/img/favicon.png") }}">
    <link href="{{ static("v2/dist/css/main.css") }}" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Fira+Sans:300,400&amp;subset=cyrillic" rel="stylesheet">

    {% block stylesheets %} {% endblock stylesheets %}
</head>

{% import "v2/_macros.jinja2" as macros %}

<body>
<div class="svg-inline">{% include "sprites/common.svg" %}</div>

{% macro render(show_submenu=True, show_notification=True, theme_class='') -%}

    <div class="navbar-container">
        {% if show_notification %}
        <div class="top-banner">
            <div class="container">
                <div class="top-banner__text">
                    {{ macros.svg_inline('top-banner-kids') }} До 14 апреля открыт <a class="top-banner__link"
                                                                                      href="/application/">приём
                    заявок</a>
                    на очное обучение в центре
                </div>
            </div>
        </div>
        {% endif %}

        <nav class="navbar{% if theme_class %} {{ theme_class }}{% endif %}">
            <div class="container">
                <button class="navbar-toggler" type="button" data-toggle="collapse"
                        data-target="#top-menu-mobile" aria-controls="top-menu-mobile"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon">{{ macros.svg_inline('burger-menu') }}</span>
                </button>
                <a class="navbar-logo" href="/v2/pages/">{{ macros.svg_inline('logo-csc') }}</a>

                <div class="menu-desktop-wrapper">
                    <ul class="navbar-nav">
                        <li class="nav-item">
                            <a class="nav-link _active" href="/v2/pages/about/">О центре</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#">Программы</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#">Курсы</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#">Онлайн</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#">Лекторий</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#">Поступление</a>
                        </li>
                    </ul>
                </div>

                <div class="menu-mobile-wrapper collapse" id="top-menu-mobile">
                    <ul>
                        <li>
                            <a class="nav-link _active" data-toggle="collapse" aria-expanded="true"
                            data-target="#top-menu-item-1"
                            aria-controls="top-menu-item-1">
                            <span>О центре</span> {{ macros.svg_inline('arrow-bottom') }}
                            </a>
                            <div class="mobile-submenu collapse show" id="top-menu-item-1">
                                <a class="mobile-submenu-item _active" href="#">Цели и история</a>
                                <a class="mobile-submenu-item" href="#">Программа</a>
                                <a class="mobile-submenu-item" href="#">Команда</a>
                                <a class="mobile-submenu-item" href="#">Преподаватели</a>
                                <a class="mobile-submenu-item" href="#">Выпускники</a>
                            </div>
                        </li>
                        <li>
                            <a class="nav-link" data-toggle="collapse" aria-expanded="false"
                               data-target="#top-menu-item-2"
                               aria-controls="top-menu-item-2">
                                <span>Программы</span> {{ macros.svg_inline('arrow-bottom') }}
                            </a>
                            <div class="mobile-submenu collapse" id="top-menu-item-2">
                                <a class="mobile-submenu-item" href="#">Software engineering</a>
                                <a class="mobile-submenu-item" href="#">Data science</a>
                                <a class="mobile-submenu-item" href="#">Computer science</a>
                            </div>
                        </li>
                        <li>
                            <a class="nav-link" href="#">Курсы</a>
                        </li>
                        <li>
                            <a class="nav-link" data-toggle="collapse" aria-expanded="false"
                               data-target="#top-menu-item-3"
                               aria-controls="top-menu-item-3">
                                <span>Онлайн</span> {{ macros.svg_inline('arrow-bottom') }}
                            </a>
                            <div class="mobile-submenu collapse" id="top-menu-item-3">
                                <a class="mobile-submenu-item" href="#">Онлайн-курсы</a>
                                <a class="mobile-submenu-item" href="#">Видеозаписи лекций</a>
                                <a class="mobile-submenu-item" href="#">Онлайн-программа</a>
                            </div>
                        </li>
                        <li>
                            <a class="nav-link" href="#">Лекторий</a>
                        </li>
                        <li class="navbar-last-item">
                            <a class="nav-link" href="#">Поступление</a>
                        </li>
                    </ul>
                </div>
                {#                            <a class="btn btn--outline btn--reg" href="#">#}
                {#                                Войти <div class="btn-reg--icon"></div>#}
                {#                            </a>#}

                <div class="dropdown">
                    <button class="btn _user-menu-btn" type="button" id="userMenuButton" data-toggle="dropdown"
                            aria-haspopup="true" aria-expanded="false">
                        <span>Святослав</span><img class="user-menu-img" src="/media/pages/index/student.png" alt="">
                    </button>
                    <div class="dropdown-menu _user-menu" aria-labelledby="userMenuButton">
                        <a class="dropdown-item" href="#">Обучение</a>
                        <a class="dropdown-item" href="#">Проекты</a>
                        <a class="dropdown-item" href="#">Курирование</a>
                        <a class="dropdown-item" href="#">Админ-панель</a>
                        <div class="dropdown-divider"></div>
                        <a class="dropdown-item" href="#">Выйти</a>
                    </div>
                </div>

            </div>
        </nav>

        {% if show_submenu %}
            <nav class="navbar navbar-submenu">
                <div class="container">
                    <ul class="navbar-nav">
                        <li class="nav-item _active">
                            <a class="nav-link" href="#">Цели и история</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#">Команда</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#">Преподаватели</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#">Выпускники</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#">Отзывы</a>
                        </li>
                    </ul>

                </div>
            </nav>

    {% endif %}
    </div>
    {% if show_notification %}<div class="top-banner-spacer"></div>{% endif %}
{%- endmacro %}

{{ render() }}

{{ render_bundle('manifest', config='V2') }}
{{ render_bundle('main', config='V2') }}
</body>
</html>